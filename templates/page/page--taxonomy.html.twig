{% if footer_style == 'footer1' %}
    <div class="footer-parallax-container">
        {% set term_machine_name = current_term.vid.value.0.target_id %}
        {% if term_machine_name == 'blog_categories' %}
            <div class="section-empty">
                <div class="container content">
                    <div class="row">
                        <div class="col-md-9 col-sm-12">
                            <div class="grid-list one-row-list">
                                <div class="grid-box row">
                                    {{ page.content_full_bar }}
                                    {{ page.content }}
                                </div>
                                                                   
                            </div>
                        </div>
                        {% if page.sidebar %}
                        <div class="col-md-3 col-sm-12 widget">
                            {{ page.sidebar }}
                        </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        {% elseif term_machine_name == 'album' %}
            <div class="section-empty">
                <div class="container content">
                    <div class="album-main">
                        <div class="cont-album-box">
                            <div class="maso-list gallery no-margins">
                                <div class="maso-box row no-margins">
                                    {{ page.content_full_bar }}
                                    {{ page.content }}
