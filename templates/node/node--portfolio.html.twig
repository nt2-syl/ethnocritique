	    <div class="col-md-8">
	        <div class="grid-list gallery">
	            <div class="grid-box row" data-lightbox-anima="fade-bottom">
	            	{% set arr_image = node.field_portfolio_image %}
	            	{% for image in arr_image %}
					    {% set imageURL_ori = file_url(image.entity.uri.value) %}
					    {% set imageURL = getUrlByImageStyle(image.entity.uri.value, 'image_600x500') %}
					    <div class="grid-item col-md-6">
		                    <a class="img-box i-center img-scale-up" href="{{ imageURL_ori }}">
		                        <i class="fa fa-camera"></i>
		                        <img src="{{ imageURL }}" alt="{{ node.getTitle() }}" />
		                    </a>
		                </div>       
					{% endfor %}
	            </div>
	        </div>
	    </div>
