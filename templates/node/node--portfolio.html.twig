            <hr class="space m" />
	        {{ content.body }}
	        <hr class="space m" />
	        <ul class="fa-ul ">
	            {% if node.field_client.0.value is not null and content.field_client is not empty %}<li><i class="fa-li im-business-man"></i><b>{{ content.field_client['#title'] }}</b> {{ content.field_client }}</li>{% endif %}

